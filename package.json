    "url": "git+https://github.com/qdhuadi/eslint-plugin-aqsc.git"
  },
  "license": "MIT",
  "author": {
    "name": "Ying Wang",
    "email": "upcwangying@gmail.com",
    "url": "https://github.com/upcwangying"
  },
  "contributors": [
    {
      "name": "Ying Wang",
      "email": "upcwangying@gmail.com",
