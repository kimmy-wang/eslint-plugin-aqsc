  "dependencies": {
    "eslint-config-aqsc": "1.0.1"
  },
  "devDependencies": {
    "eslint": "6.7.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "sort-package-json": "1.36.0",
    "standard-version": "^7.0.1"
  },
  "peerDependencies": {
    "eslint": "6.7.1"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "publishConfig": {
    "access": "public"
